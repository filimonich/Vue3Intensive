{
  "name": "vue-ssr-simple",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:client": "vue-cli-service build",
    "build:server": "vue-cli-service build --no-clean --server",
