  },
  "dependencies": {
    "@vue/server-renderer": "*",
    "axios": "*",
    "bootstrap": "*",
    "core-js": "*",
    "express": "*",
    "vue": "*",
    "vue-router": "*",
    "vuex": "*"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "*",
    "@vue/cli-plugin-router": "*",
    "@vue/cli-plugin-vuex": "*",
    "@vue/cli-service": "*",
    "@vue/compiler-sfc": "*",
    "webpack-node-externals": "*"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
